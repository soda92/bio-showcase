    "version": "0.2.0",
    "configurations": [
        {
            "name": "Update_version",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/update_version.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Django",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "runserver"
            ],
            "django": true,
            "autoStartBrowser": false,
            "program": "${workspaceFolder}\\manage.py"
        },
        {
            "name": "bio_pm",
            "type": "debugpy",
            "request": "launch",
            "module": "bio_showcase",
            "env": {
                "submodule": "bio_pm"
            }
        }
    ]
}